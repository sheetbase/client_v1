{
  "name": "@sheetbase/client",
  "version": "0.0.1",
  "description": "JS client for Sheetbase app.",
  "main": "dist/bundles/sheetbase.js",
  "module": "dist/fesm5/sheetbase.js",
  "es2015": "dist/fesm2015/sheetbase.js",
  "esm5": "dist/esm5/sheetbase.js",
  "esm2015": "dist/esm2015/sheetbase.js",
  "fesm5": "dist/fesm5/sheetbase.js",
  "fesm2015": "dist/fesm2015/sheetbase.js",
  "typings": "dist/sheetbase.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "transpile-es2015": "tsc -p tsconfig.json",
    "transpile-es5": "tsc -p tsconfig-es5.json",
    "bundle-es2015": "rollup --silent -c ",
    "bundle-es5": "rollup --silent -c rollup-es5.config.js",
    "transpile": "npm run transpile-es2015 && npm run transpile-es5",
    "bundle": "npm run bundle-es2015 && npm run bundle-es5",
    "minify": "uglifyjs dist/bundles/sheetbase.js --compress --mangle --comments --source-map -o dist/bundles/sheetbase.min.js",
    "build": "npm run transpile && npm run bundle && npm run minify",
    "lint": "tslint --project tslint.json && echo 'All files pass linting.'",
    "transpile-test": "tsc -p tests/tsconfig.test.json",
    "run-test": "nyc --cache false mocha --timeout 100000 -- build/tests/*.js",
    "test": "npm run build && npm run transpile-test && npm run run-test",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "**/tests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sheetbase/client.git"
  },
  "keywords": [
    "sheetbase",
    "google-apps-script",
    "javascript"
  ],
  "author": "Sheetbase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sheetbase/client/issues"
  },
  "homepage": "https://sheetbase.net",
  "dependencies": {
    "kyx": "file:../../../opensources/kyx/kyx-0.5.1.tgz"
  },
  "devDependencies": {
    "@sheetbase/core-server": "0.0.19",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^7.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rollup": "^0.67.4",
    "rollup-plugin-node-resolve": "^4.0.0",
    "sinon": "^7.2.2",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "uglify-js": "^3.4.9"
  }
}
