{
  "name": "@sheetbase/client",
  "version": "0.0.4",
  "description": "JS client for Sheetbase app.",
  "author": "Sheetbase",
  "homepage": "https://sheetbase.net",
  "license": "MIT",
  "main": "dist/sheetbase.js",
  "module": "dist/fesm5/sheetbase.js",
  "es2015": "dist/fesm2015/sheetbase.js",
  "esm5": "dist/esm5/sheetbase.js",
  "esm2015": "dist/esm2015/sheetbase.js",
  "fesm5": "dist/fesm5/sheetbase.js",
  "fesm2015": "dist/fesm2015/sheetbase.js",
  "typings": "dist/sheetbase.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc src --out \"docs/api\" --mode file --readme none --excludeExternals --excludeNotExported --ignoreCompilerErrors",
    "build": "tsc -p scripts/tsconfig.scripts.json && node build/scripts/build.js && npm run docs",
    "lint": "tslint --project tslint.json && echo 'All files pass linting.'",
    "test": "tsc -p tests/tsconfig.test.json && nyc --cache false mocha --timeout 100000 -- build/tests/*.js",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "**/tests"
    ]
  },
  "keywords": [
    "sheetbase",
    "google-apps-script",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sheetbase/client.git"
  },
  "bugs": {
    "url": "https://github.com/sheetbase/client/issues"
  },
  "dependencies": {
    "js-cookie": "^2.2.0",
    "localforage": "^1.7.3",
    "lscache": "^1.3.0",
    "md5": "^2.2.1",
    "pubsub-js": "^1.7.0"
  },
  "devDependencies": {
    "@sheetbase/core-server": "0.0.19",
    "@sheetbase/drive-server": "0.0.8",
    "@sheetbase/gmail-server": "0.0.6",
    "@sheetbase/sheets-server": "0.0.6",
    "@sheetbase/user-server": "0.0.5",
    "@types/chai": "^4.1.7",
    "@types/js-cookie": "^2.2.0",
    "@types/lscache": "^1.0.29",
    "@types/md5": "^2.1.33",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.15",
    "@types/pubsub-js": "^1.5.18",
    "@types/sinon": "^7.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rollup": "^0.67.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "sinon": "^7.2.2",
    "tslint": "^5.11.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.2",
    "uglify-js": "^3.4.9"
  }
}
